{
  "name": "@envato/heroku-plugin",
  "version": "0.0.1",
  "author": "Glenn Baker @gbakernet",
  "bugs": "https://github.com/envato/heroku-plugin/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^3.2.3",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/envato/heroku-plugin",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "heroku",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "envato/heroku-plugin",
  "scripts": {
    "prepublish": "npm test",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@heroku-cli/command": "^8.5.0",
    "@heroku-cli/schema": "^1.0.25",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.2",
    "tslib": "^1.14.1"
  }
}
